<script>\par
let ytPlayer;\par
\par
// YouTube API ready callback\par
function onYouTubeIframeAPIReady() \{\par
  ytPlayer = new YT.Player('ytPlayer', \{\par
    events: \{\par
      'onStateChange': onPlayerStateChange\par
    \}\par
  \});\par
\}\par
\par
// When YouTube video plays, pause background music\par
function onPlayerStateChange(event) \{\par
  const bgMusic = document.getElementById('bgMusic');\par
  if (event.data === YT.PlayerState.PLAYING) \{\par
    bgMusic.pause();\par
  \}\par
\}\par
\par
document.addEventListener('click', function(e) \{\par
  // Open Instagram post modal\par
  const p = e.target.closest('.post');\par
  if (p) \{\par
    const full = p.dataset.full || p.querySelector('img').src;\par
    const caption = p.dataset.caption || '';\par
    document.getElementById('modalImg').src = full;\par
    document.getElementById('modalCaption').textContent = caption;\par
    document.getElementById('modal').classList.add('open');\par
    document.getElementById('modal').setAttribute('aria-hidden', 'false');\par
  \}\par
\par
  // Close modal\par
  if (e.target.id === 'closeModal' || e.target.classList.contains('modal')) \{\par
    document.getElementById('modal').classList.remove('open');\par
    document.getElementById('modal').setAttribute('aria-hidden', 'true');\par
  \}\par
\});\par
\par
document.getElementById('followBtn').addEventListener('click', function() \{\par
  const btn = this;\par
  btn.classList.toggle('following');\par
  btn.textContent = btn.classList.contains('following') ? 'Following' : 'Follow';\par
\});\par
\par
document.getElementById('visitInstagram').addEventListener('click', function() \{\par
  window.open('https://www.instagram.com/alyhaamad/', '_blank', 'noopener');\par
\});\par
\par
document.getElementById('dmBtn').addEventListener('click', function() \{\par
  window.open('https://www.instagram.com/direct/t/121555572567101/', '_blank', 'noopener');\par
\});\par
\par
// Background music autoplay with enable button if blocked\par
(async () => \{\par
  const a = document.getElementById('bgMusic');\par
  try \{\par
    await a.play();\par
  \} catch (e) \{\par
    const btn = document.createElement('button');\par
    btn.textContent = '\u9654? Enable sound';\par
    Object.assign(btn.style, \{\par
      position: 'fixed', bottom: '16px', right: '16px',\par
      padding: '10px 14px', borderRadius: '10px', border: 'none',\par
      cursor: 'pointer', zIndex: 99999\par
    \});\par
    btn.onclick = async () => \{\par
      try \{\par
        await a.play();\par
        btn.remove();\par
      \} catch \{\}\par
    \};\par
    document.body.appendChild(btn);\par
  \}\par
\})();\par
\par
// Toggle media button: YouTube vs music\par
document.getElementById('toggleMedia').addEventListener('click', function() \{\par
  const bgMusic = document.getElementById('bgMusic');\par
\par
  if (ytPlayer && ytPlayer.getPlayerState() !== YT.PlayerState.PLAYING) \{\par
    bgMusic.pause();\par
    ytPlayer.playVideo();\par
    this.textContent = '\u9208? Pause Video';\par
  \} else \{\par
    if (ytPlayer) ytPlayer.pauseVideo();\par
    this.textContent = '\u9654? Play Video';\par
  \}\par
\});\par
\par
// Load YouTube API\par
let tag = document.createElement('script');\par
tag.src = "{{\field{\*\fldinst{HYPERLINK https://www.youtube.com/iframe_api }}{\fldrslt{https://www.youtube.com/iframe_api\ul0\cf0}}}}\f2\fs22 ";\par
document.body.appendChild(tag);\par
</script>\f0\par
